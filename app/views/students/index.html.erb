<h1>"listing students!!!"</h1>
<h1><% @student[0] %></h1>
<table>
    <thead>
        <tr>
            <th>"student_id"</th>
            <th>"student_name"</th>
        <tr>
    </thead>
    <tbody>
        <% @students.each do |student| %>
            <tr>
                <td><%= student.student_id %></td>
                <td><%= student.student_name %></td>
            </tr>
        <% end %>
    </tbody>
</table>

